
snippets.jar:
% content   snippets    			%content/snippets/
% skin		snippets    classic/1.0 %skin/classic/snippets/
% overlay   chrome://bluegriffon/content/xul/bluegriffon.xul		chrome://snippets/content/snippetsOverlay.xul
	content/snippets/snippetsOverlay.xul			   (content/snippetsOverlay.xul)
	content/snippets/snippetsOverlay.js 			   (content/snippetsOverlay.js)
  skin/classic/snippets/snippets-icon.png      (skin/snippets-icon.png)
  skin/classic/snippets/snippets-icon16.png    (skin/snippets-icon16.png)
  skin/classic/snippets/snippets-icon16-disabled.png    (skin/snippets-icon16-disabled.png)
  skin/classic/snippets/snippetsOverlay.css    (skin/snippetsOverlay.css)

snippets-locale.jar:
% locale    snippets    en-US		%locale/en-US/snippets/
  locale/en-US/snippets/snippetsOverlay.dtd   (locale/en-US/snippetsOverlay.dtd)
  locale/en-US/snippets/snippetsOverlay.properties   (locale/en-US/snippetsOverlay.properties)
	